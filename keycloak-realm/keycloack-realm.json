{
  "id": "myrealm",
  "realm": "myrealm",
  "enabled": true,
  "registrationAllowed": true,
  "registrationEmailAsUsername": true,
  "loginWithEmailAllowed": true,
  "verifyEmail": true,
  "resetPasswordAllowed": true,
  "duplicateEmailsAllowed": false,
  "rememberMe": true,
  "editUsernameAllowed": false,
  "sslRequired": "external",
  "authenticatorConfig": [
    {
      "alias": "otp-policy",
      "config": {
        "totp.type": "totp",
        "totp.temporary": "false",
        "totp.enabled": "true",
        "totp.policy": "HOTP,TOTP",
        "totp.hashAlgorithm": "SHA1",
        "totp.digits": "6",
        "totp.lookAheadWindow": "1",
        "totp.period": "30"
      }
    }
  ],
  "authenticationFlows": [
    {
      "alias": "browser",
      "providerId": "basic-flow",
      "topLevel": true,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "authenticator": "auth-cookie",
          "requirement": "ALTERNATIVE"
        },
        {
          "authenticator": "identity-provider-redirector",
          "requirement": "ALTERNATIVE"
        },
        {
          "authenticator": "auth-username-password-form",
          "requirement": "REQUIRED"
        },
        {
          "authenticator": "auth-otp-form",
          "requirement": "CONDITIONAL",
          "authenticatorConfig": "otp-policy"
        }
      ]
    }
  ],
  "users": [
    {
      "username": "admin",
      "email": "admin@example.com",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "admin123"
        }
      ],
      "realmRoles": ["admin"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "admin",
        "description": "Administrator role"
      },
      {
        "name": "user",
        "description": "Regular user role"
      }
    ]
  },
  "clients": [
    {
      "clientId": "my-app",
      "directAccessGrantsEnabled": true,
      "publicClient": true,
      "redirectUris": ["http://localhost:3000/*"],
      "webOrigins": ["+"]
    }
  ],
  "smtpServer": {
    "host": "smtp.gmail.com",
    "port": "587",
    "from": "no-reply@gmail.com",
    "auth": "true",
    "user": "v0t4c10not@gmail.com",
    "password": "ytmi datb mpte olny ",
    "starttls": "true"
  }
}
